    <div>
      <p>Enter into an existing room</p>
      <div id="existing-rooms"></div>
    </div>
  </div><!-- #init-panel -->
  <div id="save-panel">
  <p>This state saved in</p>
  <div id="saved-url"></div>
  </div><!-- #save -->
  <div id="load-panel">
  <p>Load state from</p>
  <div class="form-inline">
  <input id="load-id" type="text" value="" class="form-control" maxlength="16"><button id="load-send" class="btn btn-default"><i class="fa fa-sign-in"></i></button>
  </div><!-- #load-panel -->
  </div>
</div><!-- modal-panel -->
