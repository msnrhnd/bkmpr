    <div class="create-room">
      <p>Create new room</p>
        <div class="room-form">
        <div class="form-inline">
          <input id="room" type="text" value="" class="form-control" maxlength="16"><button id="sign-in" class="btn btn-default"><i class="fa fa-sign-in"></i></button>
        </div>
      </div><!-- room-form -->
