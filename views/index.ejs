<% include header %>
<div id="main-panel">
<div id="axis">
</div>
</div>
<div id="this-room"></div>
<div id="control-panel">
  <div class="form-inline">
    <input id="search" type="text" class="form-control" value="" placeholder="ISBN13" maxlength="32"><button id="plus" class="btn btn-default"><i class="fa fa-plus"></i></button><button id="save" class="btn btn-default"><i class="fa fa-toggle-down"></i></button><button id="load" class="btn btn-default"><i class="fa fa-toggle-up"></i></button><button id="sign-out" class="btn btn-default"><i class="fa fa-sign-out"></i></button>
  </div>
